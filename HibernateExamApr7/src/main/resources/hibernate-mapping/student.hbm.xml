<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="xml_based.model.Student" table="student">
        <id name="studId" column="stud_Id" length=" 5" type="int">
            <generator class="increment"/>
        </id>
        <property name="studName" column="stud_name" length="50"/>
        <property name="studRoll" column="stud_roll" length="10"/>
        <property name=""
        
    </class>
</hibernate-mapping>
